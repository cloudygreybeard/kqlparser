// String Literals - Double quoted
print "hello world"
print "with\nescape\tsequences"
print "with unicode \u0041"

// String Literals - Single quoted
print 'single quotes'
print 'with\nescape'

// String Literals - Verbatim (@ prefix)
print @"verbatim string with \n literal backslash"
print @'verbatim single quoted'

// String Literals - Multi-line (TODO: not yet supported)
// print ```multi line string```
// print ~~~alternate multiline~~~

// String Literals - Hash-prefixed (obfuscated)
print h"obfuscated"
print H'obfuscated single'
print h@"verbatim obfuscated"

// Integer Literals
print 0
print 1
print 123
print 9999999999
print 0x1A2B3C4D
print 0X1a2b3c4d
print long(42)
print int64(100)
print int(42)
print int32(100)

// Real Literals
print 1.5
print 3.14159
print 1e10
print 1E-5
print 2.5e+3
print real(1.5)
print double(2.5)
print decimal(123.456)

// Boolean Literals
print true
print false
print TRUE
print FALSE
print True
print False
print bool(true)

// DateTime Literals
print datetime(2023-01-15)
print datetime(null)

// Timespan Literals - Minutes
print 5m
print 5min
print 5minute
print 5minutes

// Timespan Literals - Seconds
print 30s
print 30sec
print 30second
print 30seconds

// Timespan Literals - Days
print 1d
print 1day
print 7days

// Timespan Literals - Hours
print 2h
print 2hour
print 24hours
print 2hr
print 2hrs

// Timespan Literals - Milliseconds
print 100ms

